{
  "title": "Gitbook的书名",
  "description": "gitbook书的描述",
  "author": "Guoze(Terry) Tang <guozet@g.clemson.edu>", 
  "language": "zh-hans",
  "gitbook": "3.2.3",
  "root": "./src",
  "links": {
    "sidebar": {
      "Home": "http://guozet.me"
    }
  },
  "plugins": [
    "theme-comscore",
    "-lunr",
    "-highlight",
    "-search",
    "-sharing",
    "search-plus",
    "disqus",
    "katex",
    "prism",
    "prism-themes",
    "github-buttons",
    "splitter",
    "sharing-plus",
    "tbfed-pagefooter",
    "expandable-chapters-small",
    "anchor-navigation-ex",
    "todo",
    "sitemap-general",
    "copy-code-button",
    "callouts",
    "toolbar-button"
  ],
  "pluginsConfig": {
    "callouts": {
      "showTypeInHeader": false
		},
    "theme-default": {
        "showLevel": true
    },
    "disqus": {
      "shortName": "guozet-me"
    }, 
    "prism": {
      "css": [
        "prism-themes/themes/prism-atom-dark.css"
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "guozetang",
          "repo": "leetcode",
          "type": "star",
          "count": true,
          "size": "small"
        }, {
          "user": "guozetang",
          "type": "follow",
          "width": "150",
          "count": false,
          "size": "small"
        }
      ]
    },
    "sharing": {
      "douban": false,
      "facebook": true,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": false,
      "qzone": false,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": true,
      "whatsapp": false,
      "all": [
        "douban",
        "facebook",
        "google",
        "instapaper",
        "line",
        "linkedin",
        "messenger",
        "pocket",
        "qq",
        "qzone",
        "stumbleupon",
        "twitter",
        "viber",
        "vk",
        "weibo",
        "whatsapp"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "By [guozetang](https://github.com/guozetang)",
      "update_label": "Updated：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "sitemap-general": {
      "prefix": "https://book.crifan.com/gitbook/gitbook_name/website/"
    },
    "toolbar-button": {
      "icon": "fa-file-pdf-o",
      "label": "PDF",
      "url": "http://book.crifan.com/books/gitbook_name/pdf/gitbook_name.pdf"
    }
  }
}