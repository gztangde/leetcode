{
  "title": "Leetcode",
  "author": "Guoze Tang",
  "description": "Leetcode",
  "links": {
    "sidebar": {
      "Home": "http://guozet.me"
    },
    "gitbook": true
  },
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "github-buttons",
    "editlink",
    "-highlight",
    "prism",
    "prism-themes",
    "bibtex-indexed-cite",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "todo",
    "copy-code-button",
    "expandable-chapters-small"
  ],
  "pluginsConfig": {
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord": ["Gitbook Use"]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "github-buttons": {
      "buttons": [{
        "user": "guozetang",
        "repo": "leetcode",
        "type": "star",
        "size": "small",
        "count": true
      }]
    },
    "prism": {
      "ignore": [
        "mermaid",
        "eval-js"
      ],
      "css": [
        "prismjs/themes/prism-solarizedlight.css"
      ]
    },
    "editlink": {
      "base": "https://github.com/guozetang/leetcode/blob/master",
      "label": "Edit",
      "multilingual": false
    },
    "page-footer-ex": {
      "copyright": "By [guozetang](https://github.com/guozetang)",
      "markdown": true,
      "update_label": "Updatedï¼š",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },

    "bibtex-indexed-cite": {
      "path": "/"
    }
  }
}